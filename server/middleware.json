{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": [
      { 
         "name": "x",
        "enabled":"false",
       "params": "$!../client",
        "methods":[
          "GET"
        ],
        "paths": [
          "/x"
        ]
        
      },
      { 
         "name": "y",
        "enabled":"false",
         "params": "$!../client/topa/hellotopa.html",
        "methods":[
          "GET"
        ],
       
        "paths": [
          "/y"
        ]
      }
    ]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "loopback#errorHandler": {}
  }
}
